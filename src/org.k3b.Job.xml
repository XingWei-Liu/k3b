<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.k3b.Job">
    <signal name="started">
    </signal>
    <signal name="canceled">
    </signal>
    <signal name="finished">
      <arg type="b" direction="out"/>
    </signal>
    <signal name="infoMessage">
      <arg type="s" direction="out"/>
      <arg type="i" direction="out"/>
    </signal>
    <signal name="progress">
      <arg type="i" direction="out"/>
    </signal>
    <signal name="subProgress">
      <arg type="i" direction="out"/>
    </signal>
    <signal name="newTask">
      <arg type="s" direction="out"/>
    </signal>
    <signal name="newSubTask">
      <arg type="s" direction="out"/>
    </signal>
    <signal name="buffer">
      <arg type="i" direction="out"/>
    </signal>
    <signal name="deviceBuffer">
      <arg type="i" direction="out"/>
    </signal>
    <signal name="nextTrack">
      <arg name="track" type="i" direction="out"/>
      <arg name="numTracks" type="i" direction="out"/>
    </signal>
    <method name="jobRunning">
      <arg type="b" direction="out"/>
    </method>
    <method name="jobDescription">
      <arg type="s" direction="out"/>
    </method>
    <method name="jobDetails">
      <arg type="s" direction="out"/>
    </method>
  </interface>
</node>
