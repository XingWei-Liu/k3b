<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.k3b.DataProject">
    <method name="createFolder">
      <arg type="b" direction="out"/>
      <arg name="name" type="s" direction="in"/>
    </method>
    <method name="createFolder">
      <arg type="b" direction="out"/>
      <arg name="name" type="s" direction="in"/>
      <arg name="dir" type="s" direction="in"/>
    </method>
    <method name="addUrl">
      <arg name="url" type="s" direction="in"/>
      <arg name="dir" type="s" direction="in"/>
    </method>
    <method name="addUrls">
      <arg name="urls" type="as" direction="in"/>
      <arg name="dir" type="s" direction="in"/>
    </method>
    <method name="removeItem">
      <arg type="b" direction="out"/>
      <arg name="path" type="s" direction="in"/>
    </method>
    <method name="renameItem">
      <arg type="b" direction="out"/>
      <arg name="path" type="s" direction="in"/>
      <arg name="newName" type="s" direction="in"/>
    </method>
    <method name="setVolumeID">
      <arg name="id" type="s" direction="in"/>
    </method>
    <method name="isFolder">
      <arg type="b" direction="out"/>
      <arg name="path" type="s" direction="in"/>
    </method>
    <method name="children">
      <arg type="as" direction="out"/>
      <arg name="path" type="s" direction="in"/>
    </method>
  </interface>
</node>
